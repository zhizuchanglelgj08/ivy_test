is.$refs.msgSuccess.toggle();\n      } else {\n        // 显示错误信息\n        // this.showWarn = true\n        this.$refs.msgWarning.toggle();\n      }\n    }\n  },\n  activated() {\n    console.log('activated');\n    \n  },\n  deactivated() {\n    console.log('deactivated');\n  }\n  // beforeRouteEnter(to, from, next) {\n  //   if (window.isLogin) {\n  //     next();\n  //   } else {\n  //     next(\"/login?redirect=\" + to.fullPath);\n  //   }\n  // }\n};\n</script>"],"sourceRoot":"src/views"}]}